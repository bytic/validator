{
  "name": "bytic/validator",
  "description": "Bytic validator",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "require": {
    "ext-json": "*",
    "egulias/email-validator": "^2.0 || ^3.0",
    "symfony/validator": "^5.0 || ^6.0"
  },
  "require-dev": {
    "bytic/phpqatools": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "ByTIC\\Validator\\": "src"
    },
    "files": [
      "src/functions/general.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\Validator\\Tests\\": "tests/src",
      "ByTIC\\Validator\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  }
}
